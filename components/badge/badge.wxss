@import '../../style/weui.wxss';
.item{
    display: inline-block;
    color: #ceacb8;
    border: 2rpx solid #ceacb8;
    border-radius: 8rpx;
    padding: 0 15rpx;
    position: relative;
    height: 36rpx;
    line-height: 36rpx;
    margin: 23rpx 30rpx;
}
.item::after{
    content: "";
    display: block;
    width: 32rpx;
    height: 32rpx;
    position: absolute;
    right: -16rpx;
    top: -16rpx;
    background: #b2b2b2 url('data:image/png;base64,') no-repeat center / 17rpx auto;
    border-radius: 50%;
}

